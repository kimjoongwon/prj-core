{
  "name": "@shared/api-client",
  "version": "1.0.0",
  "description": "Generated API client and types from OpenAPI specs",
  "keywords": [
    "api-client",
    "openapi",
    "react-query",
    "typescript",
    "axios"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start:dev": "pnpm codegen:dev",
    "build": "orval --config orval.config.js",
    "lint": "biome lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "codegen": "orval --config orval.config.js",
    "codegen:local": "NODE_ENV=local orval --config orval.config.js",
    "codegen:stg": "NODE_ENV=staging orval --config orval.config.js",
    "codegen:prod": "NODE_ENV=production orval --config orval.config.js"
  },
  "dependencies": {
    "@tanstack/react-query": "catalog:",
    "axios": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "orval": "^7.8.0",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "author": "kimjoongwon",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/prj-core.git",
    "directory": "packages/shared-api-client"
  },
  "bugs": {
    "url": "https://github.com/your-org/prj-core/issues"
  },
  "homepage": "https://github.com/your-org/prj-core/tree/main/packages/shared-api-client#readme"
}
