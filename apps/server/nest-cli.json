{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "swc",
    "assets": [
      {
        "include": "./.env",
        "outDir": "./dist"
      }
    ],
    "deleteOutDir": true,
    "typeCheck": true
  }
}
